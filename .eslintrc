{
   "extends": [
      "react-app"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      // "no-unused-vars": "warn",
      "@typescript-eslint/no-unused-vars": ["warn"],
      "indent": ["warn", 2],
      "no-useless-escape": "off"
    }
}